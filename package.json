{
  "name": "monkeytype",
  "version": "1.14.3",
  "license": "GPL-3.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "docker": "npm run docker-compose up -w @monkeytype/backend",
    "test-backend": "npm run test -w @monkeytype/backend",
    "dev": "concurrently --kill-others \"npm run dev-fe\" \"npm run dev-be\"",
    "dev-be": "npm run dev -w @monkeytype/backend",
    "dev-fe": "npm run dev -w @monkeytype/frontend",
    "live": "concurrently --kill-others \"npm run live -w @monkeytype/frontend\" \"npm run start -w @monkeytype/backend\"",
    "audit-fe": "npm run audit -w @monkeytype/frontend",
    "deploy-live": "npm run deploy-live -w @monkeytype/frontend",
    "build-fe": "npm run build-live -w @monkeytype/frontend",
    "pretty": "npm run pretty -ws",
    "pretty-code": "npm run pretty-code -ws",
    "pretty-fix": "npm run pretty-fix -ws",
    "lint": "npm run lint -ws",
    "lint-fix": "npm run lint-fix -ws",
    "pr-check-ts": "npm run pr-check-ts -w @monkeytype/frontend",
    "pr-check-build-be": "npm run build -w @monkeytype/backend",
    "pr-check-lint-json": "npm run pr-check-lint-json -w @monkeytype/frontend",
    "pr-check-quote-json": "npm run pr-check-quote-json -w @monkeytype/frontend",
    "pr-check-language-json": "npm run pr-check-language-json -w @monkeytype/frontend",
    "pr-check-other-json": "npm run pr-check-other-json -w @monkeytype/frontend"
  },
  "engines": {
    "npm": "8.1.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "concurrently": "7.2.1",
    "eslint": "8.8.0",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-require-path-exists": "1.1.9",
    "husky": "4.3.0",
    "jsonschema": "1.4.0",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.0",
    "typescript": "4.5.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  }
}
